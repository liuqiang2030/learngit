package {		import flash.display.MovieClip;	import printcenters.*;	/** 	*	Main Class	*	*	@ purpose:		Document class for movie	*/	public class Main extends MovieClip {		public function Main() {						// instantiate concrete print centers			var pcHighVol:PrintCenter = new HighVolPrintCenter();			var pcLowhVol:PrintCenter = new LowVolPrintCenter();			var pcFancy:PrintCenter = new FancyPrintCenter();						// print centers operate on different concrete print jobs			pcHighVol.print("LongThesis.doc");			pcLowhVol.print("ShortVita.doc");			pcFancy.print("SalesSpreadsheet.xls");		}	}}