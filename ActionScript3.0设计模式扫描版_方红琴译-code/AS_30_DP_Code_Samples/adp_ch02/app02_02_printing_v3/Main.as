package {		import flash.display.MovieClip;	import printcenters.*;	/** 	*	Main Class	*	*	@ purpose:		Document class for movie	*/	public class Main extends MovieClip {		public function Main() {						// the old print centers with non-parameterized 			// factory methods still work			var pcHighVol:PrintCenter = new HighVolPrintCenter();			var pcLowhVol:PrintCenter = new LowVolPrintCenter();					pcHighVol.print("LongThesis.doc");			pcLowhVol.print("ShortVita.doc");						// instantiate new print centers with 			// parameterized factory methods			var pcNewHighVol:NewPrintCenter = new NewHighVolPrintCenter();			var pcNewLowhVol:NewPrintCenter = new NewLowVolPrintCenter();								// print method operate on different print jobs			// based on passed parameters			pcNewHighVol.print("LongThesis.doc", NewHighVolPrintCenter.BW);			pcNewHighVol.print("SalesReport.pdf", NewHighVolPrintCenter.COLOR);			pcNewLowhVol.print("ShortVita.doc", NewLowVolPrintCenter.BW);			pcNewLowhVol.print("SalesChart.xlc", NewLowVolPrintCenter.COLOR);		}	}}