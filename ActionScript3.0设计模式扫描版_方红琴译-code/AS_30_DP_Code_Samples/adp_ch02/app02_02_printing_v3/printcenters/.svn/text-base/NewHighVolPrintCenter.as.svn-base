package printcenters {		public class NewHighVolPrintCenter extends NewPrintCenter {			public static const BW		:uint =  0;		public static const COLOR	:uint =  1;			override protected function createPrintjob(cKind:uint):IPrintjob {			if (cKind == BW) {				trace("Creating new printjob for the b/w workgroup printer");				return new WorkgroupPrintjob(); 			} else if (cKind == COLOR) {				trace("Creating new printjob for the color laser printer");				return new ColorLaserPrintjob(); 			} else {				throw new Error("Invalid high volume print job");				return null;			}		}	}}