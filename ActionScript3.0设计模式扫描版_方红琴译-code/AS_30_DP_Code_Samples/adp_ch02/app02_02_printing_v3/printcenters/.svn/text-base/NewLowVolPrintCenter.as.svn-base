package printcenters {		public class NewLowVolPrintCenter extends NewPrintCenter {				public static const BW		:uint =  0;		public static const COLOR	:uint =  1;			override protected function createPrintjob(cKind:uint):IPrintjob {			if (cKind == BW) {				trace("Creating new printjob for the b/w inkjet printer");				return new InkjetPrintjob(); 			} else if (cKind == COLOR) {				trace("Creating new printjob for the color inkjet printer");				return new ColorInkjetPrintjob(); 			} else {				throw new Error("Invalid low volume print job");				return null;			}		}	}}