package {   		public class Singleton	{		private var _msg:String;		private static var _instance:Singleton;		public function Singleton(pvt:PrivateClass) 		{		}		public static function getInstance():Singleton		{			if(Singleton._instance == null) 			{				Singleton._instance=new Singleton(new PrivateClass());				trace("Singleton instantiated");			}			return Singleton._instance;		}				//Get and set methods		public function getMsg():String		{			return _msg;		}		public function setMsg(alert:String):void		{			_msg = alert;		}	}}class PrivateClass{	public function PrivateClass() {	trace("PrivateClass called");	}}