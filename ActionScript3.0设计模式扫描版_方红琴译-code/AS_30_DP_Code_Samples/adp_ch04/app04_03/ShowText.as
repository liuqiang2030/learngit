package {    import flash.display.Sprite;    import flash.text.TextField;    public class ShowText extends Sprite    {        public function ShowText():void        {            var actionText:TextField=new TextField();            var msg:String="Hello ActionScript 3.ohhhhh";            actionText.text=msg;                this.addChild(actionText);            actionText.width=((msg.length)*12)+4;            actionText.height=14;            actionText.x=200;            actionText.y=150;        }    }}