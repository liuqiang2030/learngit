package{	public class Courage extends Decorator	{		var components:Component;		public function Courage(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Courage";		}		override public function good():Number		{			return 10 + components.good();		}		override public function evil():Number		{			return -8 + components.evil();		}	}}