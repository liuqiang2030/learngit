package{	public class Diligence extends Decorator	{		var components:Component;		public function Diligence(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Diligence";		}		override public function good():Number		{			return 10 + components.good();		}		override public function evil():Number		{			return -5 + components.evil();		}	}}