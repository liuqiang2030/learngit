package{	public class Hope extends Decorator	{		var components:Component;		public function Hope(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Hope";		}		override public function good():Number		{			return 5 + components.good();		}		override public function evil():Number		{			return -10 + components.evil();		}	}}