package{	public class Justice extends Decorator	{		var components:Component;		public function Justice(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Justice";		}		override public function good():Number		{			return 9 + components.good();		}		override public function evil():Number		{			return -9 + components.evil();		}	}}