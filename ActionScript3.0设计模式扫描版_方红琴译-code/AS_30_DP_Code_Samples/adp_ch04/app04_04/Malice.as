package{	public class Malice extends Decorator	{		var components:Component;		public function Malice(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Malice";		}		override public function good():Number		{			return -14 + components.good();		}		override public function evil():Number		{			return 12 + components.evil();		}	}}