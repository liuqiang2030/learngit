package{	public class Obsfuscation extends Decorator	{		var components:Component;		public function Obsfuscation(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Obsfuscation";		}		override public function good():Number		{			return -12 + components.good();		}		override public function evil():Number		{			return 7 + components.evil();		}	}}