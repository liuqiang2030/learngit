package{	public class Prejudice extends Decorator	{		var components:Component;		public function Prejudice(components:Component) 		{			this.components=components;		}		override public function getSoul():String		{			return components.getSoul() + "|Prejudice";		}		override public function good():Number		{			return -10 + components.good();		}		override public function evil():Number		{			return 15 + components.evil();		}	}}