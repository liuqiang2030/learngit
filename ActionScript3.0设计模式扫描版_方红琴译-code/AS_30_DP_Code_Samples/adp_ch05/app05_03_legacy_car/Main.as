package {		import flash.display.MovieClip;	import flash.text.*;	/** 	*	Main Class	*	@ purpose:		Document class for movie	*/	public class Main extends MovieClip {		public function Main() {			var legacyCar:LegacyCar = new LegacyCar(this.stage.stageWidth * 0.5, 								  this.stage.stageHeight * 0.75);			addChild(legacyCar);			legacyCar.start();			// rotate steering wheel 360 degrees anti-clockwise			legacyCar.setSteeringWheelAngle(-360);  		}	} }