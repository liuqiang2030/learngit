package {	public class CarAdapter extends LegacyCar implements ICar {				public function CarAdapter(xLoc:int, yLoc:int) {			super(xLoc, yLoc); // Call the superclass constructor		}				public function turnMoreToTheLeft():void {			this.setSteeringWheelAngle(nSteeringWheelAngle -= 50);		}				public function turnMoreToTheRight():void {			this.setSteeringWheelAngle(nSteeringWheelAngle += 50);		}				public function goStraight():void {			this.setSteeringWheelAngle(nSteeringWheelAngle = 0);		}			}}