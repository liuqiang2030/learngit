package {		public class Composite extends Component {			private var sName:String;		protected var aChildren:Array;				public function Composite(sNodeName:String) {			this.sName = sNodeName;			this.aChildren = new Array();		}				override public function add(c:Component):void {			aChildren.push(c);		}				override public function operation():void {			trace(this.sName);			for each (var c:Component in aChildren) {				c.operation();			}		}	}}