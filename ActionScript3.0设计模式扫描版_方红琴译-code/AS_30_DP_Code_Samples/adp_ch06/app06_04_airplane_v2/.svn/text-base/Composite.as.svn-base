package {		import flash.display.*;		public class Composite extends Sprite implements IPlane {				protected var nDamage:Number;		protected var nWeight:Number;				public function Composite(weight:Number, damage:Number = 0) {			this.nDamage = damage;			this.nWeight = weight;		}				public function getDamage():Number {			var localDamage:Number = nDamage;			for (var i:uint = 0; i < this.numChildren; i++) {				var child:DisplayObject = this.getChildAt(i);				localDamage += IPlane(child).getDamage();			}			return localDamage;		}				public function getWeight():Number {			var localWeight:Number = nWeight;			for (var i:uint = 0; i < this.numChildren; i++) {				var child:DisplayObject = this.getChildAt(i);				localWeight += IPlane(child).getWeight();			}			return localWeight;		}	}}