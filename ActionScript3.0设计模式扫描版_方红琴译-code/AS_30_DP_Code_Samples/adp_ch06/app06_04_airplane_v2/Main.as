package {		import flash.display.Sprite;	/** 	*	Main Class	*	@ purpose:		Document class for movie	*/	public class Main extends Sprite {				var airPlane:Composite;		public function Main() {						// create airplane			airPlane = new Composite(0.0);			airPlane.x = 250; airPlane.y = 100;			addChild(airPlane);						// add fuselage			var fuselage:Composite = new Fuselage(1000, 0)			airPlane.addChild(fuselage);						// add main wing on the left (port side)			var rightWing:Composite = new MainWing(200, 0);			rightWing.x = 20; rightWing.y = 50;			fuselage.addChild(rightWing);						// add main wing on the right (starbord side)			var leftWing:Composite = new MainWing(200, 0);			leftWing.scaleX = -1; // flip on vertical axis			leftWing.x = -20; leftWing.y = 50;			fuselage.addChild(leftWing);						// add engine to right wing			var rightEngine:Component = new Engine(300, 0);			rightEngine.x = 50; rightEngine.y = -20; 			rightWing.addChild(rightEngine);						// add engine to left wing			var lefttEngine:Component = new Engine(300, 0);			lefttEngine.x = 50; lefttEngine.y = -20; 			leftWing.addChild(lefttEngine);						// add tail wing on the right 			var leftTailWing:Component = new TailWing(50, 0);			leftTailWing.scaleX = -1; // flip on vertical axis			leftTailWing.x = -20; leftTailWing.y = 150;			fuselage.addChild(leftTailWing);						// add tail wing on the left 			var rightTailWing:Component = new TailWing(50, 0);			rightTailWing.x = 20; rightTailWing.y = 150;			fuselage.addChild(rightTailWing);						trace('Weight of airplane: ' + airPlane.getWeight()); // total weight of airplane					}	}}