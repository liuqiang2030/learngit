package {		import flash.events.*;		public class MainWing extends Composite {				public function MainWing(weight:Number, damage:Number = 0) {			graphics.lineStyle(25, 0x999999);			graphics.moveTo(0, 0);			graphics.lineTo(100, 0);						addEventListener(MouseEvent.CLICK, doDamage);						super(weight, damage);		}				private function doDamage(evt:Event) {			this.nDamage += 20;			trace('Damage to this wing is now ' + this.nDamage);			if (this.nDamage > 50) {				trace('Wing detached from fuselage - fatal crash!');				parent.removeChild(this);			}			evt.stopPropagation();  // stop event propegation to subsequent nodes		}	}}