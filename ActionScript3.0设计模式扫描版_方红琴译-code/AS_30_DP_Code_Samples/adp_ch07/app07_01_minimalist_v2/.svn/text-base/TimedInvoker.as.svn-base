package {		import flash.events.Event; 	import flash.events.TimerEvent; 	import flash.utils.Timer; 		class TimedInvoker extends Invoker {				public function setTimer() { 			// generate a Timer event every second 5 times (1000 ticks = 1 second)			var timer:Timer = new Timer(1000, 5); 			// register onTimerEvent as a listener method 			timer.addEventListener(TimerEvent.TIMER, this.onTimerEvent); 			timer.start();		} 				public function onTimerEvent(evt:TimerEvent):void { 			this.executeCommand();		}	}}