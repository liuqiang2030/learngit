package {		class ConcreteMacroCommand implements IMacroCommand {				var commandObjectList:Array;				public function ConcreteMacroCommand() {			this.commandObjectList = new Array();		}				public function add(c:ICommand):void {			commandObjectList.push(c);		}				public function remove(c:ICommand):void {			for (var i:int = 0; i < commandObjectList.length; i++) {				if (commandObjectList[i] === c) {					commandObjectList.splice(i, 1);					break;				}			}		}				public function execute():void {			for (var i:int = 0; i < commandObjectList.length; i++) {				commandObjectList[i].execute();			}		}	}}