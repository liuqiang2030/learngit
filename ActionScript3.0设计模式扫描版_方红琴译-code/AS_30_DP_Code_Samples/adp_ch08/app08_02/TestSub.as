package{	import flash.display.Sprite;	public class TestSub extends Sprite	{		public function TestSub()		{			var mySub:ConcreteSubject=new ConcreteSubject();			var subObserver:Observer=new ConcreteObserver();			var subObserver2:Observer=new ConcreteObserver();			var subObserver3:Observer=new ConcreteObserver();			mySub.subscribeObserver(subObserver);			//The subObserver is passed as an instance of Observer supertype			mySub.subscribeObserver(subObserver2);			mySub.subscribeObserver(subObserver3);			mySub.setLight("on");			mySub.unsubscribeObserver(subObserver);			mySub.setLight("off");		}	}}