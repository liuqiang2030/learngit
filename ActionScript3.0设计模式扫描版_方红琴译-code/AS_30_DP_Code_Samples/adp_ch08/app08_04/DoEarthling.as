package{	import flash.events.Event;	import flash.events.MouseEvent;	import flash.text.TextField;	import flash.text.TextFormat;	import flash.display.Sprite;	class DoEarthling extends Sprite	{		public function DoEarthling(earthling:Earthling,missle:Missle,alien:Alien)		{			//Earthling			var earthling:Earthling=new Earthling();			this.addChild(earthling);			earthling.x=60,			earthling.y=100;			var missle:Missle=new Missle();			this.addChild(missle);			missle.x=earthling.x+25;			missle.y=earthling.y+35;			earthling.addEventListener(MouseEvent.CLICK,fireMissle);			function fireMissle()			{				missle.play();			}			earthling.addEventListener(Event.ENTER_FRAME,earthlingHit);			function earthlingHit()			{				if(missle.hitTestObject(alien))				{					alien.rotation+=5;					torpedo.rotation=alien.rotation;					torpedo.x=alien.x-7;					torpedo.y=alien.y+17;				}			}		}	}}   