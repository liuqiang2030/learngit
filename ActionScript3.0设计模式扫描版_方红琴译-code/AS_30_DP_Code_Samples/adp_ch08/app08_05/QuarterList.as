package {	//Setup for a List Component to receive array data	import flash.display.Sprite;	import fl.controls.List;		public class QuarterList extends Sprite	{		var listArray:Array;		//Data Displayed in UI List Component		public function QuarterList(uData:Array) 		{			listArray=new Array();			listArray=uData;			var quarter_list:List=new List();			quarter_list.addItem({label:"Quarterly Results"});			for (var quarter in listArray)			{				quarter_list.addItem({label:"Q"+(quarter+1)+"="+listArray[quarter],data:listArray[quarter]});			}			quarter_list.rowCount=5;			quarter_list.width=110;			addChild(quarter_list);			quarter_list.x=50;			quarter_list.y=55;		}	}}