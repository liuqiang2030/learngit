package {	//Context class	class StringChecker	{		protected var stringWork:StringWork;		protected var sortWork:SortWork;		public function StringChecker():void		{		}		public function workStrings(s:String):String		{			return stringWork.stringer(s);		}		public function workSorts(a:Array):Array		{			return sortWork.sorter(a);		}		public function setString(sw:StringWork):void		{			stringWork=sw;		}		public function setSort(sow:SortWork):void		{			sortWork=sow;		}	}}