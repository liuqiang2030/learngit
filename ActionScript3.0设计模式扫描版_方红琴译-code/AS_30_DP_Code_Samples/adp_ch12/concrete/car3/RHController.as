package {		import flash.events.*;	import flash.ui.*;		public class RHController implements IKeyboardInputHandler {				private var model:Object;				public function RHController(oModel:Object) {			this.model = oModel;		}				public function keyPressHandler(event:KeyboardEvent):void {			switch (event.keyCode) {				case Keyboard.LEFT : 					model.addToRotationAngle(-8);					break;				case Keyboard.RIGHT : 					model.addToRotationAngle(8);					break;			}		}	}}