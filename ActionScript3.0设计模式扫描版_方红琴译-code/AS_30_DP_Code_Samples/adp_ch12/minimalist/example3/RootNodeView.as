package {		import flash.events.*;	import flash.display.*;	public class RootNodeView extends CompositeView {				public function RootNodeView(aModel:IModel, aController:IKeyboardInputHandler, target:Stage) {						super(aModel, aController);						// register to receive key press notifications			target.addEventListener(KeyboardEvent.KEY_DOWN, this.onKeyPress);		}				private function onKeyPress(event:KeyboardEvent):void {			// delegate to the controller (strategy) to handle it 			(controller as IKeyboardInputHandler).keyPressHandler(event);		}	}}