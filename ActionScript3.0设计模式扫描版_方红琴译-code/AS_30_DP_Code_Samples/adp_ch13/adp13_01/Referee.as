package {	//Abstract Class	public class Referee	{		//Move		private var p1Move:String;//Player 1's move		private var p2Move:String;//Player 2's move		private var winner:String;//Value for winner		private var outcome:String;//Describe winner		private var displayWindow:DynamicText;//Show results		private var movecheck:Array;//Array to keep track of moves		//Template Method		final function moveComplete(p1Move:String,p2Move:String,displayWindow:DynamicText,movecheck:Array):void		{			outcome=doWinner(p1Move,p2Move);			displayResults(displayWindow,outcome);			resetGame(movecheck);		}				//Abstract Methods		 protected function doWinner(p1Move:String,p2Move:String):String		{			return winner;		}		function displayResults(displayWindow:DynamicText,outcome:String):void		{			displayWindow.setMove(outcome);		}		protected function resetGame(movecheck:Array):void		{			for (var r:uint =0; r< movecheck.length; r++)			{				movecheck[r]=false;			}		}	}}